def calculation(a, b, c):
    if b == 'add':
        return a + b
    elif b == 'sub':
        return a - b
    elif b == 'mul':
        return a * b
    else:
        return a // b

n = int(input())
num = list(map(int, input().split()))
add, sub, mul, div = map(int, input().split())

min = 0
max = 0

for i in range(n-1):
    calculation(num[i], )
